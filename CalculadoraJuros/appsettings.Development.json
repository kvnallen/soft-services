{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "api1"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": true,
      "serviceUrl": "http://service-discovery:8761/eureka",
      "ValidateCertificates": false,
      "preferIpAddress":  true 
    },
    "instance": {
      "appName": "api1",
      "port": "5030",
      "hostName": "taxa-api-softplan"
    }
  }
}
